#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define LINELEN 24


void do_more(FILE *);
int see_more();

int main(int ac, char* av[])
{
  FILE *fp;

  if (ac == 1)
    do_more(stdin);
  else
    while (--ac)
      if ((fp = fopen(++av, "r")) != NULL)
      {
        do_more(fp);
        fclose(fp);
      }
  return 0;
}

void do_more(FILE *fp)
{
}

int see_more()
{
}
